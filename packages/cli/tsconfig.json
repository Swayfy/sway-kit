{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "esnext",
    "useUnknownInCatchVariables": true
  }
}
